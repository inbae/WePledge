						background-image: linear-gradient(top, rgb(244, 0, 76), rgb(229, 0, 38));
						filter: progid:DXImageTransform.Microsoft.gradient(startColorStr='#f4004c', EndColorStr='#e50026');
						color: white; font-weight:bold; width: 100%; display: block; list-style-type:none; border-left:1px solid #b8081e; text-transform:uppercase; text-align:center; -webkit-box-shadow: 0 3px #ad0316; height: 30px;line-height: 30px; text-shadow: 0 -1px rgba(0,0,0,0.2);}
		nav a:hover {
						-webkit-box-shadow: 0 3px #d8041a;
						background-color: #ff024f;
						background-image: -webkit-gradient(linear, left top, left bottom, from(rgb(255, 2, 79)), to(rgb(251, 0, 37)));

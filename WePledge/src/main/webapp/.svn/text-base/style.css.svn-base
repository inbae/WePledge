html {
	height: 100%;
}
body 						{ font-family: 'PT Sans', Helvetica, sans-serif; color: #363636; background: #d5d5d5; height: 100%; }
body>header 				{ background: rgba(0,0,0,0.6); color: #fff; background: #aaa url(css/img/banner1.png) top center no-repeat; background-size: 100% 100%; height: 130px; position: relative; }
body>header .logo			{ left: 0; position: absolute; margin-top: -10px; top: 50%; }
body>header .logo.alt		{ right: 0; left: auto; }
body>header .mobile-app		{ line-height: 40px; background: rgba(0,188,234,0.7); color: #fff;  position: absolute; width: 100%; top: 0; }
body>header .mobile-app:before	{ content:""; display:block; background: url('css/img/appIcon.png') no-repeat; height: 16px; width: 16px; position: relative; float: left; margin: 13px 10px; }
body>article, article.container { border-radius: 0 0 5px 5px; margin: 0 10px 10px 10px; background: #fff; padding: 5%; border: 1px solid #999; border-top: 0; position: relative; }

h1 							{ font-size: 2em; margin:0;font-family: 'Open Sans', serif !important; font-weight: 800; padding: 70px 0 0 30px;}
h2 							{ font-size: 1.5em; margin: 0; padding-bottom: 5px; }
h1, h2, h3, h4, h5, h6		{ font-weight: bold; }

a							{ color:  #ef1943; text-decoration: none; }
p							{ margin-bottom: 1em; line-height: 1.15em; }
strong						{ font-weight: bold; }
.icon:before				{ position: relative; display: block; float: left; margin: 4px 10px 0 0; height: 16px; width: 16px; content: ""; background: url('css/img/spritesheet.png') no-repeat; background-position: 0 -25px; }
.calendar.icon:before		{ background-position: 0 -25px; }
.location.icon:before		{ background-position: 0 -50px; }

nav				{ position: relative; display: block; bottom: 0; margin-right: 0px; z-index: 100; -webkit-box-shadow: 0 4px 6px rgba(0,0,0,.8);}
nav li 			{  display: inline-block; width:50%;  margin:0; }
nav li:first-child	{ margin-right: -6px; border-right: 1px solid blue; }
nav a 			{	background-color: #f4004c;
						background-image: -webkit-gradient(linear, left top, left bottom, from(rgb(244, 0, 76)), to(rgb(229, 0, 38)));
						background-image: -webkit-linear-gradient(top, rgb(244, 0, 76), rgb(229, 0, 38));
						background-image: -moz-linear-gradient(top, rgb(244, 0, 76), rgb(229, 0, 38));

.agenda li:nth-child(even)	{ background: #eee; -webkit-border-radius: 6px;}
.agenda li					{ margin: 0 -10px; padding: 15px 5%; position: relative; }
.agenda li p				{ margin:0; }
.agenda li time				{ position: absolute;line-height: 75px; padding-left: 10px; top: -1px; bottom: 0; right: 10px; width: 20%; text-align: center; border-left: 4px double #ccc;}
.agenda li:first-child time	{ top: -30px; padding-top: 30px; }
.agenda li:last-child time	{ bottom: -30px; bottom-top: 30px; }


.video {
	position: relative;
	padding-bottom: 60%;
	padding-top: 30px;
	margin-bottom: 20px;
	height: 0;
	overflow: hidden;
}

.video iframe {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
}

footer {
	height: 1px;
}
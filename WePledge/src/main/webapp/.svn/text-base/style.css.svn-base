						background-image: linear-gradient(top, rgb(255, 2, 79), rgb(251, 0, 37));
						filter: progid:DXImageTransform.Microsoft.gradient(startColorStr='#ff024f', EndColorStr='#fb0025');				
		}
		nav a.selected {
						-webkit-box-shadow: 0 3px #ad0316;
						background-color: #ff024f;
						background-image: -webkit-gradient(linear, left top, left bottom, from(rgb(255, 2, 79)), to(rgb(251, 0, 37)));
						background-image: -webkit-linear-gradient(top, rgb(255, 2, 79), rgb(251, 0, 37));
